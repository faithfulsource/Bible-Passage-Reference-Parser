{
  "name": "@faithfulsource/bible-passage-reference-parser",
  "version": "2.0.1",
  "description": "Identifies and parses Bible references (like John 3:16) in over 40 languages.",
  "main": "js/",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "files": [
    "js/",
    "Readme.md"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/faithfulsource/bible-passage-reference-parser.git"
  },
  "keywords": [
    "bible",
    "verses"
  ],
  "author": "Faithful Source",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/faithfulsource/bible-passage-reference-parser/issues"
  },
  "homepage": "https://github.com/faithfulsource/bible-passage-reference-parser",
  "scripts": {
    "build": "node bin/02.compile.js",
    "format": "prettier --write --ignore-path=.gitignore .",
    "format:check": "prettier --check --ignore-path=.gitignore .",
    "gen:lang": "node bin/01.add_lang.js",
    "lint": "eslint . --ignore-path .gitignore",
    "lint:fix": "eslint . --ignore-path .gitignore --fix",
    "test": "jest",
    "test:lang": "bash -c 'jest src/$0/spec.coffee'"
  },
  "devDependencies": {
    "@danielbayley/jest-preset-coffeescript": "^2.0.3",
    "coffeescript": "^1.12.7",
    "eslint": "^7.18.0",
    "eslint-config-kentcdodds": "^17.3.2",
    "jest": "^26.6.3",
    "pegjs": "^0.10.0",
    "prettier": "^2.2.1",
    "regexgen": "^1.3.0"
  }
}
